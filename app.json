{
  "expo": {
    "name": "SiiUniverse",
    "slug": "SiiUniverse",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splashScreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sii.sii"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "android": {
      "package": "com.sii.sii",
      "useNextNotificationsApi": true
    }
  }
}
